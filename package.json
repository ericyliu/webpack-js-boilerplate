{
  "name": "webpack-js-starter",
  "version": "1.0.0",
  "description": "starter code for a generic javascript app",
  "license": "MIT",
  "repository": "git@github.com:ericyliu/webpack-js-starter.git",
  "scripts": {
    "build": "webpack",
    "flow": "flow",
    "dev": "./scripts/dev.sh",
    "lint": "eslint --ext .js --config .eslintrc --fix app",
    "test": "mocha-webpack --opts ./test/mocha.opts '{,!(node_modules)/**}/*.test.js'",
    "pre-push-check": "yarn flow && yarn lint && yarn test && yarn build"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^6.0.0",
    "dotenv-webpack": "^1.5.7",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.9.1",
    "flow-babel-webpack-plugin": "^1.1.1",
    "flow-bin": "^0.75.0",
    "mocha": "^5.2.0",
    "mocha-webpack": "v2.0.0-beta.0",
    "nodemon": "^1.17.5",
    "sinon": "^6.0.1",
    "sinon-chai": "^3.2.0",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8"
  }
}
